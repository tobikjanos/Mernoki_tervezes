var c_bottomNavBarModule =
  "function bottomNavBarModule(){"
+ '    $("#allTrailsButtonDiv, #allTrailsButtonDiv a").click(function(e) {'
+ '      $("#allTrailsButtonDiv").parent().toggleClass("buttonNavBarButtonDivActive");'
+ '      location.href="/";'
+ '      e.preventDefault();'
+ '      e.stopPropagation();'
+ '    });'

+ '    $("#trailTocButtonDiv").click(function(e) {'
+ '      $("#trailTocButtonDiv").parent().toggleClass("buttonNavBarButtonDivActive");'
+ '      $("#trailTocFixedInnerDiv").html($("#trailToc").html() + "<br><br/>");'
+ '      $("#trailTocFixedDiv").toggle();'
+ '      e.preventDefault();'
+ '      e.stopPropagation();'
+ '    });'
+ '    $("#trailTocFixedCloseButton").click(function(){'
+ '      $("#trailTocFixedDiv").hide();'
+ '      $("#trailTocButtonDiv").parent().removeClass("buttonNavBarButtonDivActive");'
+ '    });'

+ '    $("#pageTocButtonDiv, #pageTocButtonDiv a").click(function(e){'
+ '      location.href="#pageToc";'
+ '      e.preventDefault();'
+ '      e.stopPropagation();'
+ '    });  '

+ '    $("#prevButtonDiv, #prevButtonDiv a").click(function(e) {'
+ '      if(prevArticleInCategory != "") {'
+ '        $("#prevButtonDiv").parent().addClass("buttonNavBarButtonDivActive");'
+ '        location.href = prevArticleInCategory;'
+ '      } else {'
+ '        alert("This is the first article in this trail.");'
+ '      }'
+ '      e.preventDefault();'
+ '      e.stopPropagation();'
+ '    });'

+ '    $("#nextButtonDiv, #nextButtonDiv a").click(function(e) {'
+ '      if(nextArticleInCategory != "") {'
+ '        $("#nextButtonDiv").parent().addClass("buttonNavBarButtonDivActive");'
+ '        location.href= nextArticleInCategory;'
+ '      } else {'
+ '        alert("This is the last article in this trail.");'
+ '      }'
+ '      e.preventDefault();'
+ '      e.stopPropagation();'
+ '    });'
+ '\n}'
;

cache("bottomNavBarModule", c_bottomNavBarModule);

